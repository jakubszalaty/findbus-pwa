{
    "name": "zditm-graphql",
    "version": "1.0.0",
    "description": "GraphQL Proxy for ZDITM API",
    "license": "MIT",
    "scripts": {
        "prestart": "rm -rf dist && tsc",
        "start": "pm2 start ./dist/index.js --node-args=\"--inspect=0.0.0.0:9229 --nolazy\" --name \"main\" --watch dist && tsc --watch",
        "restart": "pm2 restart main",
        "stop": "pm2 delete main",
        "logs": "pm2 logs main",
        "create-types": "ts-node ./scripts/create-types.ts",
        "prestart:prod": "tsc",
        "start:prod": "pm2-docker start dist/index.js",
        "host:exec": "docker exec -it $(docker ps --format \"{{.Names}}\" | grep graphql-server)",
        "host:restart": "npm run host:exec -- npm run restart",
        "host:stop": "npm run host:exec -- npm run stop",
        "host:logs": "npm run host:exec -- npm run logs",
        "host:create-types": "npm run host:exec -- npm run create-types",
        "host:shell": "npm run host:exec -- sh"
    },
    "dependencies": {
        "@nestjs/common": "^4.5.9",
        "@nestjs/core": "^4.5.10",
        "@nestjs/graphql": "^2.0.0",
        "@nestjs/microservices": "^4.5.8",
        "@nestjs/testing": "^4.5.5",
        "@nestjs/websockets": "^4.5.8",
        "apollo-server-express": "^1.3.2",
        "cors": "^2.8.4",
        "graphql": "^0.13.1",
        "graphql-import": "^0.4.3",
        "graphql-playground-middleware-express": "^1.5.6",
        "graphql-tools": "^2.21.0",
        "jsdom": "^11.9.0",
        "node-fetch": "^2.0.0",
        "ramda": "^0.25.0",
        "redis": "^2.7.1",
        "reflect-metadata": "^0.1.12",
        "rxjs": "^5.5.6",
        "subscriptions-transport-ws": "^0.9.5",
        "typescript": "^2.6.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.3",
        "@types/graphql": "^0.12.4",
        "@types/jsdom": "^11.0.4",
        "@types/node": "^9.3.0",
        "@types/node-fetch": "^1.6.7",
        "@types/ramda": "types/npm-ramda#dist",
        "graphql-code-generator": "^0.8.14",
        "ts-node": "^4.1.0"
    }
}
